{
  "version": 2,
  "name": "studioloop-prototypes",
  "buildCommand": "echo 'No build required for static files'",
  "outputDirectory": "prototypes",
  "framework": null,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/trainer-dashboard.html"
    },
    {
      "source": "/analytics",
      "destination": "/trainer-analytics.html"
    },
    {
      "source": "/schedule",
      "destination": "/trainer-schedule.html"
    },
    {
      "source": "/earnings",
      "destination": "/trainer-earnings.html"
    },
    {
      "source": "/communications",
      "destination": "/trainer-communications.html"
    },
    {
      "source": "/equipment",
      "destination": "/trainer-equipment.html"
    },
    {
      "source": "/settings",
      "destination": "/trainer-settings.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}